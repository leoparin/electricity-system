<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.leo.electricitysystem.mapper.UserMapper">

    <insert id="saveUser" useGeneratedKeys="true">
        insert into sys_user(user_name,`password`,region,user_type,create_time)
        VALUES (#{userName},#{passwd},#{region},#{userType},NOW());
    </insert>
</mapper>